/* xsh_hello.c*/

#include <xinu.h>
#include <stdio.h>

/*------------------------------------------------------------------------
 * xhs_echo - write argument strings to stdout
 *------------------------------------------------------------------------
 */
shellcmd xsh_hello(int nargs, char *args[])
{
		

	if (nargs==2) 
{
		printf("hello %s , Welcome to the world of Xinu!\n",args[1]);
}

		else if(nargs<2)

	printf("Too few arguments \n");
	else
	printf("too many arguments \n");

	

	return 0;
}
